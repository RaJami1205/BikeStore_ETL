  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Conexion_BikeStore</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Sucursal">
            <Value>=Parameters!Sucursal.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Categoria">
            <Value>=Parameters!Categoria.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Marca">
            <Value>=Parameters!Marca.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
    S.store_name AS Sucursal,
    P.product_name AS Producto,
    B.brand_name AS Marca,
    C.category_name AS Categoria,
    SUM(ISNULL(ST.quantity,0)) AS TotalInventario
FROM production.products P
INNER JOIN production.brands B
    ON P.brand_id = B.brand_id
INNER JOIN production.categories C
    ON P.category_id = C.category_id
INNER JOIN production.stocks ST
    ON P.product_id = ST.product_id
INNER JOIN sales.stores S
    ON ST.store_id = S.store_id
WHERE
   (@Sucursal IS NULL OR S.store_name LIKE '%' + @Sucursal + '%')
   AND (@Categoria IS NULL OR C.category_name LIKE '%' + @Categoria + '%')
AND (@Marca IS NULL OR B.brand_name LIKE '%' + @Marca + '%')
GROUP BY
    S.store_name,
    P.product_name,
    B.brand_name,
    C.category_name
ORDER BY
    S.store_name,
    P.product_name;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
