  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Conexion_BikeStore_DW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Cliente">
            <Value>=Parameters!Cliente.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Marca">
            <Value>=Parameters!Marca.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FechaInicio">
            <Value>=Parameters!FechaInicio.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FechaFin">
            <Value>=Parameters!FechaFin.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
    DP.CategoryName AS Categoria,
    DP.ProductName AS Producto,
    DP.BrandName AS Marca,
    CONCAT(DC.FirstName, ' ', DC.LastName) AS Cliente,
    SUM(FS.Quantity) AS CantidadVendida,
    SUM(FS.Total) AS MontoVendido
FROM FactSales FS
INNER JOIN DimProducts DP ON FS.ProductKey = DP.ProductKey
INNER JOIN DimCustomers DC ON FS.CustomerKey = DC.CustomerKey
INNER JOIN DimDate DD ON FS.OrderDateKey = DD.DateKey
WHERE
    (@Cliente IS NULL OR CONCAT(DC.FirstName, ' ', DC.LastName) LIKE '%' + @Cliente + '%')
    AND (@Marca IS NULL OR DP.BrandName LIKE '%' + @Marca + '%')
    AND (@FechaInicio IS NULL OR DD.FullDate &gt;= @FechaInicio)
    AND (@FechaFin IS NULL OR DD.FullDate &lt;= @FechaFin)
GROUP BY 
    DP.CategoryName,
    DP.ProductName,
    DP.BrandName,
    CONCAT(DC.FirstName, ' ', DC.LastName)
ORDER BY 
    DP.CategoryName,
    DP.ProductName,
    Cliente;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
